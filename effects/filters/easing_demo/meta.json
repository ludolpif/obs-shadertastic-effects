{
  "label": "Easing functions demo",
  "steps": 1,
  "input_time": true,
  "parameters": [
    {
      "name": "info",
      "value": "Easing functions helps to animate. Names and definitions are from https://easings.net/.",
      "type": "text"
    },
    {
      "name": "wanted_visual_test",
      "label": "Visual test",
      "type": "list_int",
      "values": [
        {"label": "sizes", "value": 0},
        {"label": "positions", "value": 1},
        {"label": "transparencies", "value": 2}
      ],
      "default": 0
    },
    {
      "name": "wanted_ease_func",
      "label": "Easing function",
      "type": "list_int",
      "values": [
        {"label": "linear", "value": 0},
        {"label": "Sine", "value": 1},
        {"label": "Quad", "value": 4},
        {"label": "Cubic", "value": 7},
        {"label": "Quart", "value": 10},
        {"label": "Quint", "value": 13},
        {"label": "Expo", "value": 16},
        {"label": "Circ", "value": 19},
        {"label": "Back", "value": 22},
        {"label": "Elastic", "value": 25},
        {"label": "Bounce", "value": 28}
      ],
      "default": 0
    },
    {
      "name": "wanted_ease_kind",
      "label": "Easing function kind",
      "type": "list_int",
      "values": [
        {"label": "In", "value": 0},
        {"label": "Out", "value": 1},
        {"label": "InOut", "value": 2}
      ]
    },
    {
      "name": "reverse",
      "label": "Reverse animation (t goes from 1.0 to 0.0)",
      "type": "bool",
      "default": false
    },
    {
      "name": "curve_ratio",
      "label": "Curve size",
      "type": "double",
      "slider": true,
      "min": 0.0,
      "max": 1.0,
      "default": 0.4
    },
    {
      "name": "porch",
      "label": "Delay (before and after)",
      "type": "double",
      "slider": true,
      "min": 0.0,
      "max": 1.0,
      "default": 0.3
    }
  ]
}
